<%= render 'shared/errors', obj: @organization %>
<div>
	<div>
		<%= form_for @organization do |f| %>
		<h5>Organization Name:</h5>
		<%= f.text_field :organizationName %>
		<h5>Stree Address:</h5>
		<%= f.text_field :organizationStreet %>
		<h5>City:</h5>
		<%= f.text_field :organizationCity %>
		<h5>State:</h5>
		<%= f.text_field :organizationState %>
		<h5>Zip Code:</h5>
		<%= f.text_field :organizationZip %>
		<h5>Phone Number:</h5>
		<%= f.text_field :organizationPhone%>
	
		<%= f.submit(@organization.new_record? ? "Register" : "Edit Organization") %>
		<% end %>
	</div>
</div>
