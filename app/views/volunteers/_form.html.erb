<%= render 'shared/errors', obj: @volunteer %>
<div>
	<div>
		<%= form_for @volunteer do |f| %>
		<h5>First Name:</h5>
		<%= f.text_field :firstName %>
		<h5>Last Name:</h5>
		<%= f.text_field :lastName %>
		<h5>Email:</h5>
		<%= f.email_field :emailSignIn %>
		<h5>Job Title:</h5>
		<%= f.text_field :title %>
		<h5>Phone:</h5>
		<%= f.text_field :phone %>
		<h5>Password:</h5>
		<%= f.password_field :password%>
	
		<%= f.submit(@volunteer.new_record? ? "Register" : "Edit Profile", class:"btn btn-primary") %>
		<% end %>
	</div>
</div>
